{
  "name": "schedule-service",
  "version": "1.0.0",
  "description": "Schedule Service for Student Management System",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "dev:watch": "nodemon --exec ts-node src/app.ts",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "clean": "rimraf dist"
  },
  "keywords": [
    "microservice",
    "schedule",
    "timetable",
    "management"
  ],
  "author": "Student Management Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongoose": "^7.6.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
